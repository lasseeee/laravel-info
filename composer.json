{
    "name": "lasseeee/laravel-info",
    "description": "Add support for general info in Laravel projects",
    "type": "library",
    "require": {
        "graham-campbell/markdown": "^13.0",
        "laracasts/flash": "^3.1"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Lasseeee\\Info\\InfoServiceProvider"
            ],
            "aliases": {
                "Markdown": "\\GrahamCampbell\\Markdown\\Facades\\Markdown"
            }
        }
    },
    "autoload": {
        "psr-4": {
            "Lasseeee\\Info\\": "src"
        }
    }
}
